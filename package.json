{
  "name": "unpkg-bundler",
  "version": "0.0.4",
  "description": "",
  "types": "lib",
  "main": "lib/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ChristopherHButler/unpkg-bundler"
  },
  "bugs": {
    "url": "https://github.com/ChristopherHButler/unpkg-bundler/issues"
  },
  "homepage": "https://github.com/ChristopherHButler/unpkg-bundler",
  "keywords": [
    "bundler",
    "client-side-bundler",
    "esbuild",
    "unpkg",
    "unpkg-bundler"
  ],
  "files": [
    "build/**/*"
  ],
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json",
    "dev": "esbuild index.ts --bundle '--define:process.env.NODE_ENV=\"development\"' --outdir=build-dev",
    "build": "esbuild index.ts --bundle '--define:process.env.NODE_ENV=\"production\"' --minify --outdir=build"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "esbuild-wasm": "^0.8.36",
    "localforage": "^1.9.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0"
  }
}
